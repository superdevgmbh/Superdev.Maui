<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:s="http://schemas.superdev.ch/dotnet/2021/maui">

    <!-- Picker -->
    <Style
        x:Key="PickerStyle_Merge"
        TargetType="Picker">
        <Setter Property="s:DialogExtensions.NeutralButtonText" Value="{s:Translate CancelButtonText}" />
        <Setter Property="s:DialogExtensions.DoneButtonText" Value="{s:Translate DoneButtonText}" />
    </Style>

    <!-- Entry -->
    <Style
        x:Key="EntryStyle_Merge"
        TargetType="Picker">
        <Setter Property="s:DialogExtensions.DoneButtonText" Value="{s:Translate DoneButtonText}" />
    </Style>

    <!-- Editor -->
    <Style
        x:Key="EditorStyle_Merge"
        TargetType="Picker">
        <Setter Property="s:DialogExtensions.DoneButtonText" Value="{s:Translate DoneButtonText}" />
    </Style>

</ResourceDictionary>