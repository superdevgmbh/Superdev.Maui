<?xml version="1.0" encoding="utf-8" ?>

<ContentPage
    x:Class="SuperdevMauiDemoApp.Views.WebViewDemoPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:s="http://schemas.superdev.ch/dotnet/2021/maui"
    xmlns:vm="clr-namespace:SuperdevMauiDemoApp.ViewModels"
    x:DataType="vm:WebViewDemoViewModel"
    Title="WebViewDemoPage">
    <Grid>
        <s:CustomWebView
            Headers="{Binding Headers}"
            NavigatedCommand="{Binding NavigatedCommand}"
            NavigatingCommand="{Binding NavigatingCommand}"
            Source="{Binding Url}" />
    </Grid>
</ContentPage>