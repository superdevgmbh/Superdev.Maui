<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary
    x:Class="Superdev.Maui.Styles.ThemeFontResources"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!-- Font Families -->
    <OnPlatform
        x:Key="Theme.FontFamily.Monospace"
        x:TypeArguments="x:String">
        <On
            Platform="Android"
            Value="monospace" />
        <On
            Platform="iOS"
            Value="Courier New" />
    </OnPlatform>


</ResourceDictionary>